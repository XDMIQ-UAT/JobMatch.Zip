      "script": "python E:\\agents\\groups\\engineers\\precheck-agent.py",
      "python_method": "run_alert_agent_prechecks()"
    },
    "run_project_prechecks": {
      "description": "Run general project pre-checks",
      "script": "python E:\\agents\\groups\\engineers\\precheck-agent.py",
      "python_method": "run_project_prechecks()"
    },
    "check_twilio_backend": {
      "description": "Check if Twilio backend is configured and accessible",
      "script": "python E:\\agents\\groups\\engineers\\precheck-agent.py",
      "python_method": "check_twilio_backend()"
    },
    "check_python_package": {
      "description": "Check if a Python package is installed",
      "script": "python E:\\agents\\groups\\engineers\\precheck-agent.py",
      "python_method": "check_python_package(package_name)"
