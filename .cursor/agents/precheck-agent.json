    "check_file_exists": {
      "description": "Check if a required file exists",
      "script": "python E:\\agents\\groups\\engineers\\precheck-agent.py",
      "python_method": "check_file_exists(file_path)"
    },
    "check_network_endpoint": {
      "description": "Check if a network endpoint is accessible",
      "script": "python E:\\agents\\groups\\engineers\\precheck-agent.py",
      "python_method": "check_network_endpoint(url, timeout)"
